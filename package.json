{
  "name": "@slimio/error",
  "version": "0.2.0",
  "description": "Slim.IO - Opinionated Error(s) handle/generator",
  "main": "index.js",
  "scripts": {
    "prepublishOnly": "pkg-ok",
    "test": "cross-env psp && ava --verbose",
    "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
    "coverage": "nyc npm test",
    "report": "nyc report --reporter=html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SlimIO/Config.git"
  },
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "husky": {
    "hooks": {
      "pre-push": "cross-env eslint index.js && npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "engines": {
    "node": ">=12"
  },
  "keywords": [
    "SlimIO",
    "JSON",
    "error"
  ],
  "author": "SlimIO",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SlimIO/Config/issues"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": false,
    "lines": 98,
    "statements": 98,
    "functions": 98,
    "branches": 90
  },
  "homepage": "https://github.com/SlimIO/Config#readme",
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@escommunity/minami": "^1.0.0",
    "@slimio/eslint-config": "^3.0.3",
    "@slimio/psp": "^0.9.2",
    "ava": "^2.4.0",
    "cross-env": "^5.2.1",
    "eslint": "^6.3.0",
    "husky": "^3.0.4",
    "jsdoc": "^3.6.3",
    "nyc": "^14.1.1",
    "pkg-ok": "^2.3.1"
  },
  "dependencies": {
    "clean-stack": "^2.2.0"
  }
}
